		System.out.print("user_inter_fase: ");
		String user_inter_fase =  sane_a_user.nextLine();
		System.out.println();
		boolean bn1 = true;
		
		Point pt1 = new Point(120,160);
		System.out.print("boolean: " + bn1);
		
		
		
		switch (user_inter_fase){
			case "ojudsonleo":
				System.out.print("boolean: " + bn1);
				
