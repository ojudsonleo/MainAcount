package pro1.my_2_java_program;

public class Dog {
	
	private String name;
	private int age;
	
	public Dog(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	public void speak() {
		System.out.println("i am " + this.name + " and I am " + this.age + " years old" + " and i am a dog");
	}
}
